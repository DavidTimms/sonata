
fn main(| input) {
	dimentions: fn readDetails(details dimens) {
		if details.count < 2
			dimens
		else
			readDetails(details.slice(2) 
				addKey(dimens, details(0), Number(details(1))))
	}(input {C: 90})
	inRadians = forIn(dimentions fn(key, value) {
		if key.toUpperCase() = key 
			addKey({} key degToRad(value))
		else
			addKey({} key value)
	}).reduce(mix)

	
}

fn degToRad(deg) (deg * Math.PI) / 180